{
  "name": "itfy-evoting-backend",
  "version": "1.1.0",
  "description": "The backend for the itfy-evoting software",
  "main": "app.js",
  "directories": {
	  "doc":"docs"
  },
  "scripts": {
	  "dev": "nodemon",
	  "start": "babel-node src/app.js",
	  "build": "babel src -d dist",
	  "test": "jest",
	  "prelint": "eslint src",
	  "lint": "eslint src --fix",
	  "seed": "babel-node src/database/seeders/app.seeder.js" 
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 20000,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "setupFiles": [
      "dotenv/config"
    ],
    "verbose": true
  },
  "keywords": [
    "evoting",
    "itfy",
    "vote"
  ],
  "author": "John Ametepe Agboku",
  "license": "ISC",
  "dependencies": {
    "agenda": "^5.0.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "joi": "^18.0.1",
    "joi-objectid": "^4.0.2",
    "jsonwebtoken": "^9.0.2",
    "mongo-sanitize": "^1.1.0",
    "mongoose": "^8.19.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.11",
    "redoc-express": "^2.1.3",
    "sharp": "^0.34.5",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/node": "^7.28.0",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.38.0",
    "@faker-js/faker": "^10.1.0",
    "babel-jest": "^30.2.0",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "mongodb-memory-server": "^10.2.3",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4"
  }
}
